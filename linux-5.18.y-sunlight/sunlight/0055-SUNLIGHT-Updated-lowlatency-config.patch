 sunlight/gcc/config-sunlight-lowlatency | 4 +++-
 1 file changed, 3 insertions(+), 1 deletion(-)

diff --git a/sunlight/gcc/config-sunlight-lowlatency b/sunlight/gcc/config-sunlight-lowlatency
index ba67a995866c..76b2c928b948 100644
--- a/sunlight/gcc/config-sunlight-lowlatency
+++ b/sunlight/gcc/config-sunlight-lowlatency
@@ -1,6 +1,6 @@
 #
 # Automatically generated file; DO NOT EDIT.
-# Linux/x86_64 5.18.5 Kernel Configuration
+# Linux/x86_64 5.18.6 Kernel Configuration
 #
 CONFIG_CC_VERSION_TEXT="gcc (Debian 11.3.0-3) 11.3.0"
 CONFIG_CC_IS_GCC=y
@@ -185,6 +185,7 @@ CONFIG_ARCH_SUPPORTS_NUMA_BALANCING=y
 CONFIG_ARCH_WANT_BATCHED_UNMAP_TLB_FLUSH=y
 CONFIG_CC_HAS_INT128=y
 CONFIG_CC_IMPLICIT_FALLTHROUGH="-Wimplicit-fallthrough=5"
+CONFIG_GCC12_NO_ARRAY_BOUNDS=y
 CONFIG_ARCH_SUPPORTS_INT128=y
 # CONFIG_NUMA_BALANCING is not set
 CONFIG_CGROUPS=y
@@ -11180,6 +11181,7 @@ CONFIG_CRYPTO_LIB_SM3=m
 CONFIG_CRYPTO_LIB_SM4=m
 # end of Crypto library routines
 
+CONFIG_LIB_MEMNEQ=y
 CONFIG_CRC_CCITT=y
 CONFIG_CRC16=y
 CONFIG_CRC_T10DIF=y
